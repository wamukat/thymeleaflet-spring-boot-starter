{
  "name": "thymeleaflet-spring-boot-starter",
  "version": "1.0.0",
  "description": "Tailwind CSS build system for Thymeleaflet Spring Boot Starter",
  "private": true,
  "scripts": {
    "build": "tailwindcss -i src/main/resources/thymeleaflet/input.css -o src/main/resources/META-INF/resources/static/css/thymeleaflet.css --minify",
    "build:dev": "tailwindcss -i src/main/resources/thymeleaflet/input.css -o src/main/resources/META-INF/resources/static/css/thymeleaflet.css",
    "watch": "tailwindcss -i src/main/resources/thymeleaflet/input.css -o src/main/resources/META-INF/resources/static/css/thymeleaflet.css --watch",
    "clean": "rm -f src/main/resources/META-INF/resources/static/css/thymeleaflet.css",
    "postinstall": "npm run build"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.9",
    "@tailwindcss/typography": "^0.5.15",
    "tailwindcss": "^3.4.17"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "tailwindcss",
    "spring-boot",
    "thymeleaf",
    "css",
    "frontend"
  ],
  "author": "thymeleaflet-spring-boot-starter",
  "license": "MIT",
  "dependencies": {
    "playwright": "^1.55.0"
  }
}
